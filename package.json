{
  "name": "lumen-link",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Unified crypto trading bot merging TypeScript architecture with Python API integrations - supports paper/live execution, backtesting, and comprehensive risk controls",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "tsx src/index.ts",
    "paper": "MODE=paper tsx src/index.ts",
    "backtest": "tsx src/backtester/run.ts",
    "backtest:history": "tsx src/backtester/history.ts",
    "backtest:compare": "tsx src/backtester/compare.ts",
    "fetch-history": "tsx src/data/fetchHistory.ts",
    "cdp:create-evm-account": "tsx src/agents/createCdpEvmAccount.ts",
    "mcp:coinbase": "tsx src/agents/coinbaseMcpServer.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@aws-sdk/client-bedrock-runtime": "^3.995.0",
    "@aws-sdk/client-secrets-manager": "^3.995.0",
    "@coinbase/agentkit": "^0.10.4",
    "@coinbase/agentkit-model-context-protocol": "^0.2.0",
    "@coinbase/cdp-sdk": "^1.44.1",
    "@coinbase/onchainkit": "^1.1.2",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.13.5",
    "better-sqlite3": "^11.7.0",
    "ccxt": "^4.5.39",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "jsonrepair": "^3.13.2",
    "node-cron": "^4.2.1",
    "openai": "^6.22.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "rss-parser": "^3.13.0",
    "simple-statistics": "^7.8.8",
    "technicalindicators": "^3.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.19.11",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
